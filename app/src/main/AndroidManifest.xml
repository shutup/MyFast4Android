<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shutup.myfast4android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main_menu.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".common.BaseActivity" />
        <activity android:name=".listview_adapter_demo.ListViewAdapterDemoActivity" />
        <activity android:name=".recycler_view.RecyclerViewDemoActivity" />
        <activity android:name=".cardview.CardViewDemoActivity"></activity>
    </application>

</manifest>
